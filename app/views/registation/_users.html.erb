<div class="group-view">
<% @users.each do |user| %>
    <div class="card">
        <p><%= user.first_name%></p>
        <p><%= user.last_name%></p>
        <p><%= user.birthdate%></p>
        <p><%= user.email%></p>
        <p><%= user.phone%></p>
        <p><%= user.gender%></p>
        <p><%= user.subject%></p>
    </div>
    
<% end %>
</div>